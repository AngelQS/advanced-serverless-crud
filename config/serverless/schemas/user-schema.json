{
  "$schema": "https://json-schema.org/draft-04/schema#",
  "title": "User",
  "type": "object",
  "required": ["name", "age"],
  "properties": {
    "nombre": {
      "description": "The person's first name.",
      "type": "string"
    },
    "age": {
      "description": "Age in years which must be equal to or greater than zero.",
      "type": "integer",
      "minimum": 18
    }
  }
}
